{
  "extends": "standard",
  "rules": {
    "no-duplicate-imports": 0,
    "semi": [2, "never"],
    "space-before-function-paren": 0,
    "func-names": 0,
    "no-throw-literal":0,
    "no-cond-assign":0,
    "no-new-func":0,
    "no-unmodified-loop-condition":0
  },
  "globals": {
    "_": true,
    "angular":true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}
