{
  "name": "myangular",
  "version": "0.0.1",
  "description": "learn es6 karma jasmine webpack",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "build": "npm run build_dev && npm run build_pro",
    "build_dev": "./node_modules/.bin/webpack --config webpack.config.js",
    "build_pro": "./node_modules/.bin/webpack --config webpack.config.production.js",
    "test": "./node_modules/karma/bin/karma start --single-run --no-auto-watch karma.config.js",
    "test_watch": "./node_modules/karma/bin/karma start --auto-watch --no-single-run karma.config.js",
    "lint": "eslint src/**"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shengxinjing/myAngular.git"
  },
  "keywords": [
    "angular",
    "es6",
    "babel",
    "webpack",
    "karma"
  ],
  "author": "shengxinjing",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shengxinjing/myAngular/issues"
  },
  "homepage": "https://github.com/shengxinjing/myAngular",
  "dependencies": {
    "babel-runtime": "^5.1.10",
    "jquery": "^2.1.4",
    "lodash": "^4.11.1",
    "sinon": "git+https://github.com/uberVU/Sinon.JS"
  },
  "devDependencies": {
    "babel-core": "^5.8.0",
    "babel-eslint": "^3.1.6",
    "babel-loader": "^5.4.0",
    "casperjs": "^1.1.0-beta5",
    "eslint": "^2.12.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-config-standard": "^5.3.1",
    "eslint-config-vue": "^1.0.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.4.2",
    "eslint-plugin-react": "^5.1.1",
    "istanbul-instrumenter-loader": "^0.1.3",
    "imports-loader":"^0.6.5",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.8",
    "karma-chrome-launcher": "^0.2.0",
    "karma-coverage": "^0.5.0",
    "karma-firefox-launcher": "^0.1.6",
    "karma-ie-launcher": "^0.2.0",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-safari-launcher": "^0.1.1",
    "karma-spec-reporter": "0.0.20",
    "karma-webpack": "^1.7.0",
    "phantomjs": "^1.9.17",
    "phantomjs-polyfill": "0.0.1",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  }
}
